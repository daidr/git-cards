{
    "version": 2,
    "name": "git-cards",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        },
        {
            "src": "/static/(.*)",
            "dest": "/public/$1"
        }
    ]
}